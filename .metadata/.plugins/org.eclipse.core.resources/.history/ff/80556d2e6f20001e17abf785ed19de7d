package com.aurionpro.model;

public class Account {
	private int id;
	private String name;
	private int balance;
	private AccountType accountType;
	
	public Account(int id, String name, int balance, AccountType accountType) {
		this.id = id;
		this.name = name;
		this.balance = balance;
		this.accountType = accountType;
	}

}
